{
  "name": "aws-sso-extender",
  "description": "Extension for AWS SSO (Identity Center) users",
  "author": "WTFender",
  "url": "https://wtfender.com/posts/aws-sso-extender",
  "private": true,
  "version": "1.1.6",
  "type": "module",
  "scripts": {
    "watch:chrome": "VITE_EXT_DEBUG=true vite --mode watch:chrome --watch build",
    "watch:firefox": "VITE_EXT_DEBUG=true vite --mode watch:firefox --watch build",
    "build:chrome": "vue-tsc && vite --mode build:chrome build",
    "build:firefox": "vue-tsc && vite --mode build:firefox build && web-ext build -s ./dist/firefox/ -a ./release/firefox/ --overwrite-dest"
  },
  "dependencies": {
    "primeicons": "^6.0.1",
    "primevue": "^3.26.1",
    "vue": "^3.2.47",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "sass": "^1.60.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-web-extension": "^3.0.1",
    "vue-tsc": "^1.2.0",
    "web-ext": "^7.6.0"
  }
}
